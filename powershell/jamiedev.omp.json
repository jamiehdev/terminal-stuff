{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#C56CF0",
          "foreground": "#2F3542",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .Env.CUSTOM_USERNAME }}@{{ .Env.CUSTOM_HOSTNAME }}",
          "type": "session"
        },
        {
          "background": "#7D5FFF",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#5352ED",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#3742FA",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#C56CF0",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}